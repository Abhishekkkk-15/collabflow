{
  "name": "@collabflow/workers",
  "version": "1.0.0",
  "private": true,
  "main": "dist/index.js",
  "scripts": {
    "dev:workspace": "ts-node src/workspace/index.ts",
    "dev:invite": "ts-node src/invite/index.ts",
    "dev:email": "ts-node src/email/index.ts",
    "dev:notification": "ts-node src/notification/index.ts",
    "dev:all": "concurrently \"pnpm dev:workspace\" \"pnpm dev:invite\" \"pnpm dev:email\"",
    "build": "tsc -p tsconfig.json",
    "start:workspace": "node dist/workspace/index.js",
    "start:invite": "node dist/invite/index.js",
    "start:email": "node dist/email/index.js",
    "start:notification": "node dist/notification/index.js",
    "start:all": "concurrently \"pnpm start:workspace\" \"pnpm start:invite\" \"pnpm start:email\"",
    "dev": "ts-node src/index.ts",
    "start": "node dist/index.js"
  },
  "dependencies": {
    "@collabflow/db": "workspace:*",
    "@collabflow/types": "workspace:*",
    "bullmq": "5.65.0",
    "ioredis": "5.8.2",
    "resend": "^6.6.0"
  },
  "devDependencies": {
    "concurrently": "^7.0.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.9.3"
  }
}
